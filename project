#include <iostream>
using namespace std;
int main(){
    int withdrawl_amount{};
    const double tax {0.5};
    const double total_amount{120};
    cin>>withdrawl_amount;
    if (withdrawl_amount>total_amount){
        cout<<total_amount<<endl;
    }
    else{
        if (withdrawl_amount%5==0){
            cout<<(total_amount - (withdrawl_amount + tax))<<endl;
        }
        else {
            cout<<total_amount<<endl;
        }
    }
    return 0;
}